{
  "name": "ngx-translate-cache",
  "version": "0.1.1",
  "description": "ngx-translate extension to facilitate language cache.",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ngx-translate-cache -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\"",
    "test": "tsc && karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jgpacheco/ngx-translate-cache"
  },
  "author": {
    "name": "Jaime",
    "email": "jaime.glez.pacheco@gmail.com"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/username/repo/issues"
  },
  "devDependencies": {
    "@angular/common": "^4.4.7",
    "@angular/compiler": "^4.4.7",
    "@angular/compiler-cli": "^4.4.7",
    "@angular/core": "^4.4.7",
    "@angular/platform-browser": "^4.4.7",
    "@angular/platform-browser-dynamic": "^4.4.7",
    "@compodoc/compodoc": "^1.1.7",
    "@ngx-translate/core": "^8.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.14.2",
    "angular-in-memory-web-api": "^0.3.2",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.6.1",
    "core-js": "^2.6.3",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.4.0",
    "gulp-rollup": "^2.16.2",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.4.0",
    "node-sass": "^4.11.0",
    "node-sass-tilde-importer": "^1.0.2",
    "node-watch": "^0.5.9",
    "protractor": "~5.1.0",
    "rollup": "^0.49.3",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.5.12",
    "systemjs": "^0.20.12",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0",
    "zone.js": "^0.8.29"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
